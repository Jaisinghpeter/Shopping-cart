{{outlet}}
<div class="container">
    <div id="login" class="row">
        <div class="col-sm-4"></div>
        <form > 
            <div class="form-group">
                <label for="userName">User Name:</label>
                {{input type="text" required=true value=userName class="form-control" id="userName"}}
            </div>
            <div class="form-group">
                <label for="password">Password:</label>
                {{input type="password" required=true  value=password class="form-control" id="password"}}
            </div>
            <div>
                <LinkTo @route="register" >No Account Click to Register</LinkTo><br><br>
            </div>
            <button type="submit" {{action "checklogin"}} class="btn btn-primary">Submit</button>
        </form  >
    </div>
</div>
{{#each model as |user|}}
{{user.email}}
{{/each}}

